{
  "name": "mcc-ai-dashboard",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:server": "node --watch server.js",
    "dev:client": "vite",
    "dev:full": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy": "npm run firebase:deploy",
    "deploy:preview": "npm run firebase:build && firebase hosting:channel:deploy preview",
    "firebase:build": "npm run build && cd functions && npm run build",
    "firebase:serve": "npm run firebase:build && firebase emulators:start",
    "firebase:deploy": "npm run firebase:build && firebase deploy",
    "firebase:deploy:hosting": "npm run build && firebase deploy --only hosting",
    "firebase:deploy:functions": "cd functions && npm run build && firebase deploy --only functions"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/xlsx": "^0.0.35",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase": "^11.8.1",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.3.2",
    "localforage": "^1.10.0",
    "lucide-react": "^0.344.0",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.22.3",
    "styled-jsx": "^5.1.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.0",
    "@types/jest": "^29.5.12",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "firebase-tools": "^14.5.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
